

<div class="container">

<% if @company_profile.errors.any? %>

    <div class="alert alert-danger" role="alert">
      <h2>
        <%= pluralize(@company_profile.errors.count,"error") %> occured
      </h2>
      <ul>
        <% @company_profile.errors.full_messages.each do |e| %>
            <li>
              <%= e %>
            </li>
        <% end %>
      </ul></div>
<% end %>
