<div class="fields">
<tr>
  <td><div class="ui-widget">
    <%= f.collection_select :item_id, Item.order(:name),:id, :name, {prompt: "Select Item"}, {id:"combobox1"} %>
  </div></td>
  <td><%= f.text_field :unit_sell_price, :class => "form-control", :placeholder => "Unit"  %></td>
  <td><%= f.text_field :quantity, :class => "form-control", :placeholder => "Price"  %></td>
  <td><%= f.text_field :cash_credit , :class => "form-control", :placeholder => "Total" %>
  </td>
  <%= f.hidden_field :_destroy %>
